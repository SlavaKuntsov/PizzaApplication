<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:vm="clr-namespace:Pizza.MVVM.ViewModel"
	xmlns:view="clr-namespace:Pizza.MVVM.View">

	<DataTemplate
		DataType="{x:Type vm:HomeViewModel}">
		<view:HomeView />
	</DataTemplate>

	<DataTemplate
		DataType="{x:Type vm:CatalogViewModel}">
		<view:CatalogView />
	</DataTemplate>
	
	<!--<DataTemplate
		DataType="{x:Type vm:SearchViewModel}">
		<view:SearchView />
	</DataTemplate>-->

</ResourceDictionary>